{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended", "schedule:weekly", "group:allNonMajor"],
  "rangeStrategy": "bump",
  "labels": ["dependencies"],
  "dependencyDashboard": true,
  "rebaseWhen": "conflicted",
  "vulnerabilityAlerts": {
    "labels": ["security"],
    "enabled": true
  }
}
